{
    "autosu": true,
    "colorcolumn": 120,
    "colorscheme": "one-dark",
    "diffgutter": true,
    "eofnewline": true,
    "fastdirty": true,
    "filemanager": true,
    "filemanager.compressparent": true,
    "filemanager.foldersfirst": true,
    "filemanager.openonstart": false,
    "filemanager.showdotfiles": true,
    "filemanager.showignored": true,
    "ignorecase": false,
    "mkparents": true,
    "pluginchannels": [
        "https://raw.githubusercontent.com/micro-editor/plugin-channel/master/channel.json"
    ],
    "pluginrepos": [],
    "savecursor": true,
    "softwrap": true,
    "statusformatl": "$(filename) $(modified) $(status.paste) ",
    "statusformatr": "| $(line):$(col) | $(opt:filetype) | $(opt:encoding) | $(opt:fileformat) | keybinds:alt-g help:ctl-g ",
    "tabstospaces": true
}
