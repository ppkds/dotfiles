# ls aliases
    alias la='ls -A'
    alias lla='ls -alFh'
    alias ll='ls -lFh'

# File and Directory aliases
    alias mv='mv -i'
    alias cp='cp -iv'
    alias rm='rm -v'
    alias mkdir='mkdir -pv'

# Convinience aliasies
    alias apt='sudo apt'
    alias bashrc='source ~/.bashrc'
    alias cat="batcat"
    alias cd-git='cd /home/ppkds/Dev/Git/'
    alias cls='clear'
    alias df='df -hT'
    alias edit-git-dotfiles='vim -p    \
            ~/Dev/Git/dotfiles/bashrc                       \
            ~/Dev/Git/dotfiles/bashrc_aliases               \
            ~/Dev/Git/dotfiles/config/micro/bindings.json   \
            ~/Dev/Git/dotfiles/config/micro/settings.json   \
            ~/Dev/Git/dotfiles/config/neofetch/config.conf  \
            ~/Dev/Git/dotfiles/export_mod                   \
            ~/Dev/Git/dotfiles/onehalfdark.vim              \
            ~/Dev/Git/dotfiles/vimrc                        \
            ~/Dev/Git/dotfiles/README.md' 
    alias free='free -ht'
    alias neofetch='clear && echo && neofetch'
    alias pwgen='pwgen -y 10'
    alias python='clear && python3'
    alias resources='neofetch && echo && echo - Memory - && free && echo && echo - Disk - && echo && df | grep -v /dev/loop | grep -v tmpfs'
    alias top='htop'
    alias weather='echo && curl wttr.in/{,London,Chicago,Mumbai}?format=3 && echo'
    alias weechat='weechat-curses'      # custom alias for weecha
    alias wget='wget -c'
