# ls aliases
    # alias ls='exa --group-directories-first'
    alias la='ls -Ah'
    alias ll='ls -CF -lh'
    alias lla='ls -alCFh'

# File and Directory aliases
    alias mv='mv -i'
    alias cp='cp -iv'
    alias rm='rm -v'
    alias mkdir='mkdir -pv'

# Convinience aliasies
    alias apt='sudo apt'
    alias bashrc='source ~/.bashrc'
    alias cat="batcat"
    alias cls='clear'
    alias df='df -hT'
    alias edit-git-dotfiles='vim -p    \
            /home/ppkds/Dev/Git/dotfiles/bashrc                       \
            /home/ppkds/Dev/Git/dotfiles/bashrc_aliases               \
            /home/ppkds/Dev/Git/dotfiles/config/micro/bindings.json   \
